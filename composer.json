{
    "name": "yceruto/cqs-bundle",
    "description": "Symfony integration for Command Query Separation pattern",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Yonel Ceruto",
            "email": "yonelceruto@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "symfony/http-kernel": "^7.0",
        "symfony/dependency-injection": "^7.0",
        "yceruto/cqs": "dev-main",
        "symfony/config": "^7.0"
    },
    "require-dev": {
        "doctrine/orm": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Yceruto\\CqsBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yceruto\\CqsBundle\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev"
}
